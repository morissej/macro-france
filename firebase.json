{
    "hosting": {
        "target": "competitivite-explorer-fr",
        "public": "out",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "cleanUrls": true,
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}